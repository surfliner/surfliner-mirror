{
  "extends": [
    "config:base",
    ":preserveSemverRanges"
  ],
  "gitlabci": {
    "fileMatch": ["^ci/.*\\.ya?ml$"]
  },
  "bundler": { "enabled": true },
  "labels": [ "dependencies" ],
  "regexManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": ["ARG RUBY_VERSION=(?<currentValue>.*?)\n" ],
      "depNameTemplate": "ruby",
      "datasourceTemplate": "docker"
    }
  ]
}
